<h1>売上一覧・取り込み</h1>

<table border="1">
  <thead>
    <tr>
      <th>店舗コード</th>
      <th>売上</th>
    </tr>
  </thead>

  <tbody>
    <% @sales.each do |sale| %>
      <tr>
        <td><%= sale.shop_id %></td>
        <td><%= sale.rate %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= form_tag import_sales_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "インポート" %>
<% end %>

<a class="btn" href="/companies">一覧へ戻る</a>
